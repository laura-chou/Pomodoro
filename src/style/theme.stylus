#theme
  height 100vh
  font-size 20px
  .data-table
    height: 100%
    background rgba(248,248,255,0.5)
    table
      tr
        th
          background-color #FFFFCD
        td
          vertical-align: middle !important
          border-top 1px solid #ADADAD
          &:nth-child(2)
            color: #000